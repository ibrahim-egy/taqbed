<%- include('partials/header') %>

        <div class="data__wrapper">
            <a href="/allOwners"><img src="../images/search.svg" alt="oil well icon" class="icon-svg"></a>
            <form action="/data", method="post">
                <input id="ownerName" list="owners" type="search" name='name' placeholder="الاسم الكامل" autofocus>
                <datalist id="owners">
                    <% owners.forEach(owner => { %>
                        <option class="dropdown-element"><%=owner%></option>
                    <% }) %>
                </datalist>
                <button class="button button__light" type="submit">Search</button>
            </form>
            <div class="data__action-buttons action__buttons">

                <form action="/add" class="side">
                    <button class="button button-dark button-lg" type="submit">Add</button>
                </form>
                <form action="/logout" method="post" class="side">
                    <button class="button button-lg" type="submit">Logout</button>
                </form>
                <form action="/deletedList" class="side">
                    <button class="button button-sage button-lg" type="submit">Deleted Users</button>
                </form>
            </div>
        </div>


    <%- include('partials/footer') %>